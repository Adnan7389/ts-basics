{
    "compilerOptions": {
        "target": "es2019",
        "module": "es2020",
        "moduleResolution": "node",
        "rootDir": "./src",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "preserveConstEnums": true,
        "declaration": false
    },
    "include": [
        "src"
    ]
}